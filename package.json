{
  "name": "my-qwik-basic-starter",
  "devDependencies": {
    "@builder.io/qwik": "^1.19.0",
    "@builder.io/qwik-city": "^1.19.0",
    "@eslint/js": "latest",
    "@types/node": "20.19.0",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.19.0",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "^7.20.0",
    "vercel": "^50.9.6",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "description": "Demo app with sample routes",
  "engines": {
    "bun": ">=1.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "qwik check-client src dist && vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint --max-warnings 0 \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "type": "module",
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "gray-matter": "^4.0.3",
    "rehype-highlight": "^7.0.2",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  }
}
